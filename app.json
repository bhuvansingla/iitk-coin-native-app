{
  "expo": {
    "name": "IITK-Coin",
    "slug": "IITK-Coin",
    "version": "1.0.0",
    "orientation": "portrait",
    "entryPoint": "./src/App.tsx",
    "icon": "./src/assets/icon.png",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "icon": "./src/assets/ios-icon.png",
      "bundleIdentifier": "com.iitkcoin.iitkcoin"
    },
    "android": {
      "package": "com.iitkcoin.iitkcoin",
      "versionCode": 1
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    }
  }
}
